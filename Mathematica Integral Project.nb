Cell[TextData[{
 "To find ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "(", 
    SubscriptBox["x", "k"], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "6d86fdfb-b00e-4fd7-b749-52c3e0a5e9d5"],
